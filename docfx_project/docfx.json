{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "AdministradorService/AdministradorService.csproj",
            "ContactosService/ContactosService.csproj",
            "ExtractoService/ExtractoService.csproj",
            "PagosService/PagosService.csproj",
            "PayTollApp.DataAccess/PayTollApp.DataAccess.csproj",
            "PayTollApp.Models/PayTollApp.Models.csproj",
            "PayTollApp.SharedServices/PayTollApp.SharedServices.csproj",
            "RecargasService/RecargasService.csproj",
            "ShortcutService/ShortcutService.csproj",
            "SolicitudesService/SolicitudesService.csproj",
            "TarjetasService/TarjetasService.csproj",
            "UsuariosService/UsuariosService.csproj",
            "VehiculosService/VehiculosService.csproj"
          ],
          "cwd": ".."
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
